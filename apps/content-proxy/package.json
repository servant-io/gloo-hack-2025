{
  "name": "content-proxy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3002",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:local:up": "docker-compose up -d",
    "db:local:down": "docker-compose down",
    "db:generate:migration": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "db:push": "drizzle-kit push --config drizzle.config.ts",
    "db:seed": "tsx db/seed/index.ts",
    "db:seed:content": "tsx db/seed/content.ts",
    "db:seed:personalization": "tsx db/seed/personalization.ts",
    "db:reset": "tsx db/reset/index.ts",
    "db:reset:content": "tsx db/reset/content.ts",
    "db:reset:personalization": "tsx db/reset/personalization.ts"
  },
  "dependencies": {
    "@phosphor-icons/react": "catalog:",
    "ajv": "catalog:",
    "dotenv": "catalog:",
    "drizzle-kit": "catalog:",
    "drizzle-orm": "catalog:",
    "lodash": "catalog:",
    "next": "catalog:",
    "pg": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@eslint/eslintrc": "catalog:",
    "@tailwindcss/postcss": "catalog:",
    "@types/node": "catalog:",
    "@types/pg": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "bun": "catalog:",
    "eslint": "catalog:",
    "eslint-config-next": "catalog:",
    "tailwindcss": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
